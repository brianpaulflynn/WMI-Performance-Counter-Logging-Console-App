<?xml version="1.0"?>
<configuration>
  <connectionStrings>
    <add name="DbRepository" connectionString="data source=DbRepositoryServer;Initial Catalog=MyDB;Integrated Security=SSPI;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="DwImportProc" value="PerfCounterDwImport_sp" />
    <add key="SleepTimeMillisecions" value="15000" />
    <add key="PerfMon01" value="DbRepository;PerfCounterInsert_sp;MyWebServer;ASP.NET Apps v2.0.50727;Requests Executing;" />

    <add key="PerfMon02" value="DbRepository;PerfCounterInsert_sp;MyWebServer;ASP.NET Apps v2.0.50727;Requests In Application Queue;" />
    <add key="PerfMon03" value="DbRepository;PerfCounterInsert_sp;MyWebServer;ASP.NET Apps v2.0.50727;Pipeline Instance Count;" />
    <add key="PerfMon04" value="DbRepository;PerfCounterInsert_sp;MyWebServer;ASP.NET Apps v2.0.50727;Request Wait Time;" />
    <add key="PerfMon05" value="DbRepository;PerfCounterInsert_sp;MyWebServer;ASP.NET Apps v2.0.50727;Request Execution Time;" />

    <add key="PerfMon06" value="DbRepository;PerfCounterInsert_sp;MyWebServer;.NET CLR LocksAndThreads;Current Queue Length;w3wp;" />
    <add key="PerfMon07" value="DbRepository;PerfCounterInsert_sp;MyWebServer;.NET CLR LocksAndThreads;Contention Rate / sec;w3wp;" />
    <add key="PerfMon08" value="DbRepository;PerfCounterInsert_sp;MyWebServer;.NET CLR LocksAndThreads;# of current logical Threads;w3wp;" />
    <add key="PerfMon09" value="DbRepository;PerfCounterInsert_sp;MyWebServer;.NET CLR LocksAndThreads;# of current physical Threads;w3wp;" />

    <add key="PerfMon10" value="DbRepository;PerfCounterInsert_sp;MyWebServer;Process;Thread Count;w3wp;" />

  </appSettings>
  <system.diagnostics>
        <sources>
            <!-- This section defines the logging configuration for My.Application.Log -->
            <source name="DefaultSource" switchName="DefaultSwitch">
                <listeners>
                    <add name="FileLog"/>
                    <!-- Uncomment the below section to write to the Application Event Log -->
                    <!--<add name="EventLog"/>-->
                </listeners>
            </source>
        </sources>
        <switches>
            <add name="DefaultSwitch" value="Information"/>
        </switches>
        <sharedListeners>
            <add name="FileLog" type="Microsoft.VisualBasic.Logging.FileLogTraceListener, Microsoft.VisualBasic, Version=8.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a, processorArchitecture=MSIL" initializeData="FileLogWriter"/>
            <!-- Uncomment the below section and replace APPLICATION_NAME with the name of your application to write to the Application Event Log -->
            <!--<add name="EventLog" type="System.Diagnostics.EventLogTraceListener" initializeData="APPLICATION_NAME"/> -->
        </sharedListeners>
    </system.diagnostics>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
